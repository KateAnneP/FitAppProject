<AnchorPane prefHeight="450.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fitappproject.DodawaniePotrawViewController">
   <children>
      <Pane layoutX="5.0" layoutY="5.0" prefHeight="440.0" prefWidth="340.0">
